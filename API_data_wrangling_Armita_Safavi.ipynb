   "source": [
    "#What was the median trading volume during this year\n",
    "\n",
    "def median_f(list):\n",
    "    s_list = sorted(list)\n",
    "    n = len(list)\n",
    "    int(n)\n",
    "    if n% 2 == 1:\n",
    "        return '${:,.2f}'.format(s_list[(n-1)//2])\n",
    "    else:\n",
    "        return '${:,.2f}'.format((s_list[(n-1)//2]+ s_list[(n//2)])//2)"
   ]
