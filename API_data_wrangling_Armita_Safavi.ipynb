   "source": [
    "#What was the largest change in any one day (based on High and Low price)?\n",
    "\n",
    "def change(list):\n",
    "    changes = []\n",
    "    for i in range(0,len(list)):\n",
    "            c = list[i][2] - list[i][3]\n",
    "            changes.append(c)\n",
    "    sort_c = sorted(changes)\n",
    "    return sort_c[-1]\n"
   ]
